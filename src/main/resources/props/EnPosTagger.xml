<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties version="1.0">

	<!-- Tagger name -->
	<entry key="taggerName">ENPOS</entry>

	<!-- Saves the vector features of the training instances into a input file 
		for liblinear C implementation; if set to true, then no training will be 
		performed; only the input file for liblinear will be created -->
	<entry key="saveModelInputFile">false</entry>

	<!-- Store wrong tag assignment in file resources/eval/*.debug -->
	<entry key="debug">false</entry>

	<!-- Liblinear settings -->
	<entry key="solverType">MCSVM_CS</entry>
	<entry key="c">0.1</entry>
	<entry key="eps">0.3</entry>

	<!-- Control parameters -->
	<entry key="windowSize">2</entry>
	<entry key="numberOfSentences">-1</entry>
	<entry key="dim">0</entry>
	<entry key="subSamplingThreshold">0.000000001</entry>

	<!-- features (not) activated -->
	<entry key="withWordFeats">false</entry>
	<entry key="withShapeFeats">true</entry>
	<entry key="withSuffixFeats">true</entry>
	<entry key="withClusterFeats">true</entry>

	<entry key="WordSuffixFeatureFactory.ngram">false</entry>

	<!-- HERE ARE THE MAIN CORPUS BASED SETTINGS -->

	<!-- Corpus access parameters: Index of relevant tags in conll format -->
	<entry key="wordFormIndex">1</entry>
	<entry key="posTagIndex">4</entry>

	<!-- CORPUS parameters - labeled data -->
	<entry key="trainingLabeledData">
		resources/data/sancl-2012/sancl.labeled/ontonotes-wsj-train.conll
	</entry>
	<entry key="devLabeledData">
		resources/data/sancl-2012/sancl.labeled/ontonotes-wsj-dev.conll,
		resources/data/pbiotb/dev/english_pbiotb_dev.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-answers-dev.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-emails-dev.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-newsgroups-dev.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-reviews-dev.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-weblogs-dev.conll
	</entry>
	<entry key="testLabeledData">
		resources/data/sancl-2012/sancl.labeled/ontonotes-wsj-test.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-answers-test.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-emails-test.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-newsgroups-test.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-reviews-test.conll,
		resources/data/sancl-2012/sancl.labeled/gweb-weblogs-test.conll
	</entry>

	<!-- CORPUS parameters - unlabeled data -->
	<entry key="trainingUnLabeledData">
		resources/data/sancl-2012/sancl.labeled/ontonotes-wsj-train-sents.txt,
		resources/data/pbiotb/dev/english_pbiotb_dev-sents.txt
	</entry>
	<entry key="devUnLabeledData">
		resources/data/sancl-2012/sancl.labeled/ontonotes-wsj-dev-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-answers-dev-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-emails-dev-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-newsgroups-dev-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-reviews-dev-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-weblogs-dev-sents.txt,
		resources/data/ptb/unlab/english_ptb_unlab,
		resources/data/pbiotb/unlab/all-unlab.txt,
		resources/data/sancl-2012/sancl.all/gweb-answers.unlabeled.txt,
		resources/data/sancl-2012/sancl.all/gweb-emails.unlabeled.txt,
		resources/data/sancl-2012/sancl.all/gweb-newsgroups.unlabeled.txt,
		resources/data/sancl-2012/sancl.all/gweb-reviews.unlabeled.txt,
		resources/data/sancl-2012/sancl.all/gweb-weblogs.unlabeled.txt
	</entry>
	<entry key="testUnLabeledData">
		resources/data/sancl-2012/sancl.labeled/ontonotes-wsj-test-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-answers-test-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-emails-test-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-newsgroups-test-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-reviews-test-sents.txt,
		resources/data/sancl-2012/sancl.labeled/gweb-weblogs-test-sents.txt
	</entry>

	<!-- Cluster file -->
	<entry key="clusterIdSourceFileName">/Users/gune00/data/Marmot/Word/en_marlin_cluster_1000</entry>

	<!-- Training file -->
	<!-- it is redundant, because also set in trainingLabeledData; however, 
		there as a list of training files, which is actually currently not supported 
		when calling the GNTrainer and GNTtagger -->
	<entry key="trainingFile">resources/data/sancl-2012/sancl.labeled/ontonotes-wsj-train.conll
	</entry>

</properties>